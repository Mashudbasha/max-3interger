lass GFG {
  
    static String LexicographicalMaxString(String str)
    {
        
        String mx = "";
        for (int i = 0; i < str.length(); ++i) {
            if (mx.compareTo(str.substring(i)) <= 0) {
                mx = str.substring(i);
            }
        }
  
        return mx;
    }
  
   
    public static void main(String[] args)
    {
        String str = "banana";
        System.out.println(LexicographicalMaxString(str));
    }
}
