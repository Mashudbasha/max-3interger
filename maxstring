package com;
class Maxstring {
  
    public static String MaxString(String str)
    {
        
        String mx = "";
        
		String str1 = null;
        for (int i = 0; i < str1.length(); ++i) {
            if (mx.compareTo(str1.substring(i)) <= 0) {
                mx = str1.substring(i);
            }
        }
  
        return mx;
    }
  
   
    public static void main(String[] args)
    {
        String str = "banana";
        System.out.println(MaxString(str));
    }
}
